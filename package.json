    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@babel/core": "7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "clean-css": "^5.2.2",
    "critical": "^5.0.4",
    "cssesc": "^3.0.0",
    "del-cli": "^4.0.1",
    "dotenv": "^10.0.0",
    "eleventy-plugin-embed-tweet": "^0.3.3",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "focus-trap": "^6.7.1",
    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
