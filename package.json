{
  "name": "eleventastic",
  "version": "1.0.0",
  "description": "A boilerplate for eleventy websites",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "NODE_ENV=production run-s clean eleventy:prod --print-label",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "del-cli dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/maxboeck/eleventastic"
  },
  "author": "Max BÃ¶ck",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-css": "^4.2.3",
    "critical": "^1.3.10",
    "cssesc": "^3.0.0",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "eleventy-plugin-embed-tweet": "^0.2.0",
    "focus-trap": "^5.1.0",
    "focus-visible": "^5.1.0",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.20",
    "luxon": "^1.24.1",
    "markdown-it": "^10.0.0",
    "medium-zoom": "^1.0.5",
    "memfs": "^3.2.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "svg-sprite": "^1.5.0",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2"
  }
}
