    "luxon": "^2.0.2",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-footnote": "^3.0.3",
    "medium-zoom": "^1.0.6",
    "memfs": "^3.3.0",
    "node-fetch": "^3.0.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "sanitize-html": "^2.5.3",
    "svg-sprite": "^1.5.3",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    
